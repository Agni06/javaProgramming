class main extends test
{

  public static void main(String[] args)
{

 Scanner sc = new Scanner(System.in);
test t1 = new test();

t1.x= sc.nextDouble();
t1.y = sc.nextDouble();

test t2 = new test();

t2.x = sc.nextDouble();
t2.y = sc.nextDouble();
main obj = new main();
obj.distance(t1,t2);


}
}

class test
{

double x,y;
double distance(test t1 , test t2 )
{

double result  = Math.sqrt(Math.pow((t2.x-t1.x),2))-(Math.pow(t2.y-t1.y),2);
return result ;
}
}
